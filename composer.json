{
    "name": "altauth/secret-phrase-generator",
    "description": "Secret Phrase Generator for Laravel.",
    "keywords": [
        "secret",
        "phrase",
        "generator"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.65"
    },
    "authors": [
        {
            "name": "Roman Dukhanin",
            "email": "romandukhanin@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "AltAuth\\SecretPhraseGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AltAuth\\SecretPhraseGenerator\\SecretPhraseGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}